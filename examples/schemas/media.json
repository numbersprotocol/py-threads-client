{
    "$id": "https://example.com/media.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Media",
    "type": "object",
    "properties": {
        "dataHash": {
            "type": "string",
            "description": "The media's data hash"
        },
        "dataCID": {
            "type": "string",
            "description": "The media's data CID"
        },
        "dataFilename": {
            "type": "string",
            "description": "The media's data filename"
        },
        "metadataHash": {
            "type": "string",
            "description": "The media's metadata hash"
        },
        "metadataCID": {
            "type": "string",
            "description": "The media's metadata CID"
        },
        "metadataFilename": {
            "type": "string",
            "description": "The media's metadata filename"
        }
    }
}
